<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel='stylesheet' href='/stylesheets/style.css'/>
    </head>
    <body>
        <% include includes/navbar.ejs %>

        <h1><%= title %></h1>
        <p>Welcome to <%= title %>, Global (local) data printing <%= points %></p>

        <h3><b><%= videodata.categoryName %></b></h3>
        <ul>
            <% videodata.categories.forEach(function (item) { %>
            <li><%= item.categoryName %> - <%= item.categoryDescription %></li>
            <% }) %>
        </ul>
    </body>
</html>